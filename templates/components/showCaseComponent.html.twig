<div{{attributes}}>
	{% if factory %}
		<div class="container_showcases">
			<div class="container_homepage_show_case" id="initial">
				<div class="navigation_show_case">
					<div class="title">
						<h2>{{factory.name}}</h2>
					</div>
					<div class="content_navigation">
						<div class="left_part">
							<div class="image">
								<img src="/images/{{factory.imageName[0]}}" alt="factory image">
							</div>
						</div>
						<div class="right_part">
							<div class="links">
								<a href="#about">A propos</a>
								<a href="#services">Services</a>
								<a href="#produits">Produits</a>
								<a href="#contact">Contact</a>
							</div>
						</div>
					</div>
				</div>
				{% set homePage = this.sectionHomePage() %}
				{% if homePage is defined %}
					<section class="container_showcase_main_section">
						{% for item in homePage %}
							<div class="content_main_section">
								<div class="background_image">
									<img src="/images/{{item.imageName[0]}}" alt="showcase image">
								</div>
								<div class="titles_main">
									<div class="title1">
										<h1>{{factory.name}}</h1>
									</div>
									<div class="title2">
										<h2>{{item.titre1}}</h2>
									</div>
									<div class="title3">
										<h3>{{item.titre2}}</h3>
									</div>
									<div class="contact_us">
										<a href="mailto:{{factory.email}}">Prendre contact</a>
									</div>
								</div>
							</div>
						{% endfor %}
					</section>
				{% endif %}

				{% set about = this.sectionAbout() %}
				{% if about is not empty %}
					<section class="container_showcase_first_section" id="about">
						<div class="title">
							<h2>Quid ?
							</h2>
							<div class="line"></div>
						</div>


						{% for item in about %}
							<div class="content_section">
								<div class="left_content_section">
									<p>{{item.description|nl2br}}</p>
								</div>
								<div class="right_content_section">
									<div class="image">
										<img src="/images/{{item.imageName[0]}}" alt="showcase image">
									</div>
								</div>
							</div>
						{% endfor %}
					</section>
				{% endif %}


				{% set images = this.sectionImages() %}
				{% if images is not empty %}
					<section class="container_showcase_second_section">
						<div
							class="container_images">
							{# <div class=" swiper-wrapper"> #}
							{% for item in images %}
								<div class="image swiper-slide">
									<img src="/images/{{item.imageName[0]}}" alt="showcase image">
								</div>
							{% endfor %}
						</div>
						<div class="see_more">
							<button data-action="live#action" data-live-action-param="incrementImages">
								<span>Voir plus</span>
								<svg xmlns="http://www.w3.org/2000/svg" width="20px" height="20px" viewbox="0 0 16 16" fill="none">
									<path fill-rule="evenodd" clip-rule="evenodd" d="M0 8L3.07945 4.30466C4.29638 2.84434 6.09909 2 8 2C9.90091 2 11.7036 2.84434 12.9206 4.30466L16 8L12.9206 11.6953C11.7036 13.1557 9.90091 14 8 14C6.09909 14 4.29638 13.1557 3.07945 11.6953L0 8ZM8 11C9.65685 11 11 9.65685 11 8C11 6.34315 9.65685 5 8 5C6.34315 5 5 6.34315 5 8C5 9.65685 6.34315 11 8 11Z" fill="#000000"/>
								</svg>
							</button>
						</div>
					</section>

				{% endif %}

				{% set news = this.sectionNews %}
				{% if news is not empty %}
					<section class="top_news_section">
						<div class="title">
							<h2>Du nouveau
							</h2>
							<div class="line"></div>
						</div>


						<div class="container_news_section">
							{% for item in news %}
								<div class="news_section">
									<div class="image">
										<img src="/images/{{item.imageName[0]}}" alt="showcase image">
									</div>
									<div class="description">
										<div class="header">
											<h4>{{item.title}}</h4>
										</div>
										<div class="content">
											<p>{{item.description|nl2br}}</p>
										</div>
									</div>
								</div>
							{% endfor %}
						</div>
						<div class="see_more">
							<button data-action="live#action" data-live-action-param="incrementNews">
								<span>Voir plus</span>
								<svg xmlns="http://www.w3.org/2000/svg" width="20px" height="20px" viewbox="0 0 16 16" fill="none">
									<path fill-rule="evenodd" clip-rule="evenodd" d="M0 8L3.07945 4.30466C4.29638 2.84434 6.09909 2 8 2C9.90091 2 11.7036 2.84434 12.9206 4.30466L16 8L12.9206 11.6953C11.7036 13.1557 9.90091 14 8 14C6.09909 14 4.29638 13.1557 3.07945 11.6953L0 8ZM8 11C9.65685 11 11 9.65685 11 8C11 6.34315 9.65685 5 8 5C6.34315 5 5 6.34315 5 8C5 9.65685 6.34315 11 8 11Z" fill="#000000"/>
								</svg>
							</button>
						</div>
					</section>
				{% endif %}


				{% set services = this.sectionServices %}
				{% if services is not empty %}
					<section class="container_services_section" id="services">
						<div class="title">
							<h2>Profitez de nos services
							</h2>
							<div class="line"></div>
						</div>
						<div class="services_section">
							{% for item in services %}
								<div class="services">
									<div class="left_part">
										<h3>{{item.title}}</h3>
										<div class="content">
											<p>{{item.description|nl2br}}</p>
										</div>
									</div>
									<div class="right_part">
										<div class="image">
											<img src="/images/service.jpg" alt="">
										</div>
									</div>
								</div>
							{% endfor %}
						</div>
					</section>
				{% endif %}

				{% set products = this.sectionProducts %}
				{% set productsCategories = this.sectionproductsCategories %}
				{% if products is not empty %}
					<section class="container_services_section" id="produits">
						<div class="title">
							<h2>Nos produits
							</h2>
							<div class="line"></div>
						</div>

						<div class="product_category">
							{% for item in productsCategories %}
								<button data-action="live#action" data-live-action-param="setCategoryProduct" data-live-id-param="{{item.id}}">{{item.name}}</button>
							{% endfor %}
						</div>
						<div class="products_section">
