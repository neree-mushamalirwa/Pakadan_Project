				<section class="container_services_section" id="contact">
					<div class="title">
						<h2>Prenez contact
						</h2>
						<div class="line"></div>
					</div>

					<div class="container_contact">
						<form action="{{path('app_contact')}}" method="post">
							<input type="number" name="factoryId" id="" value="{{factory.id}}" style="display:none;">
							<div class="group_input">
								<div class="input">
									<label for="">Noms *</label>
									<input type="text" name="noms" id="" required>
								</div>
							</div>
							<div class="group_input">
								<div class="input">
									<label for="">Email *</label>
									<input type="email" name="email" id="" required>
								</div>
								<div class="input">
									<label for="">Téléphone *</label>
									<input type="text" name="telephone" id="" required>
								</div>
							</div>
							<div class="input">
								<label for="">Sujet du message</label>
								<input type="text" name="subject" id="" required>
							</div>
							<div class="input">
								<label for="">Votre message</label>
								<textarea name="message" id="message" required></textarea>
							</div>

							<div class="input_submit">
								<button type="submit">Envoyer</button>
							</div>
						</form>
					</div>
				</section>

